.box{
  width: 710rpx;
  margin: 20rpx;
  border-radius: 15rpx;
  background-color: white;
  padding: 15rpx 20rpx;
}
.top{
  width: 100%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 30rpx;
  font-weight: 600;
}
.middle{
  width: 95%;
  background-color: #efefee;
  border-radius: 15rpx;
  padding: 15rpx 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 26rpx;
}
.middle view{
  margin: 10rpx 0;
}
.btm{
  width: 99%;
  margin: 15rpx 0 0 0;
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  justify-content: space-between;
}
.btm1{
  width: 99%;
  margin: 0rpx 0 0 0;
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  justify-content: space-between;
}
